<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mit.teamnull.mapper.PartnerMapper">
	
	<select id="getList" resultType="mit.teamnull.domain.PartnerVO">
		<![CDATA[
			SELECT * FROM partner
		]]>
	</select>
	
	<select id="insert">
		INSERT INTO partner
		(part_name,part_address,part_tel,part_rep,part_manager)
		VALUES(#{part_name},#{part_address},#{part_tel},#{part_rep},#{part_manager})
	</select>
	
<!-- 	<insert id="insertSelectKey">
		<selectKey order="BEFORE" keyProperty="part_name" resultType="String"></selectKey>
		INSERT INTO partner
		(part_name,part_address,part_tel,part_rep,part_manager)
		VALUES(#{part_name},#{part_address},#{part_tel},#{part_rep},#{part_manager})
	</insert> -->
	
	<select id="read" resultType="mit.teamnull.domain.PartnerVO">
		SELECT * FROM partner WHRER part_name = #{part_name}
	</select>
	
	<delete id="delete">
		DELETE * FROM partner WHRER part_name = #{part_name}
	</delete>
	
	<select id="update">
		UPDATE partner 
		SET part_name=#{part_name},
		part_address=#{part_address},
		part_tel=#{part_tel},
		part_rep=#{part_rep},
		part_manager=#{part_manager})
		WHERE part_name = #{part_name}
	</select>



</mapper>